<div class="logincontent">
  <%= image_tag 'login.jpg', class:"loginimage" %>
  <div class="loginform">
    <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div>
        <%= f.input :email,
                    required: false,
                    autofocus: true,
                    input_html: { autocomplete: "email" } %>
        <%= f.input :password,
                    required: false,
                    input_html: { autocomplete: "current-password" } %>
        <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
      </div>

      <div>
        <%= f.button :submit, "Log In", class: "loginbutton jeju" %>
      </div>

      <div>
          <%= render "devise/shared/links", class:"loginbutton" %> 
      </div>
    <% end %>
    
    
  </div>
</div>


